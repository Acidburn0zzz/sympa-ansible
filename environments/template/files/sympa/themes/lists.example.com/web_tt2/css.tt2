/* header and menu*/
[% PROCESS "css/fonts_css.tt2" %]


/****/


* {		/* Proportional fonts */
    font-family: 'LatoWeb', sans-serif;
    font-size: 1rem;
}

body {
	display: table;
	width: 100%;
	height: 100%;
	position: absolute;
	color: [% color_6 %];
	font-family: 'LatoWeb', sans-serif;
	font-size: 0.85em;
	line-height: 1;
	background-color: transparent;
	padding-bottom: 80px;
	z-index:1;
	-webkit-text-size-adjust:100%;
}

.background{
	display:block;
	position:fixed;
	width:100vw;
	height:100vh;
	padding:0;
	margin:0;
	background-image: url('[% static_content_url %]/lists.example.com/icons/background/background.jpg');
	background-repeat: no-repeat;
	background-attachment: fixed;
	background-size:cover;
	z-index:-1;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)";
    filter: alpha(opacity=50);
    -webkit-filter: opacity(0.5);
    -moz-filter: opacity(0.5);
    -o-filter: opacity(0.5);
    -ms-filter: opacity(0.5);
    filter: opacity(0.5);

}

main {
	[% IF top_menu %]
	min-height:450px;
	[% ELSE %]
	min-height:550px;
	[% END %]
	margin-top: 1rem;
	margin-bottom:3rem;
	padding: 0.5rem 2rem 0 2rem;
	background-color: rgba(255,255,255,0.8);
	box-shadow: 0px 10px 20px 0px rgba(1, 1, 1, 0.6);
	text-align:left;
    line-height:1.7;
    animation-name: theFade; /* nom de l'animation */
    animation-duration: 1s; /* temps de l'animation (4s) */
    animation-iteration-count: 1; /* nombre de boucles (infinie) */
    animation-timing-function: linear; /* accelération de l'animation (constant) */
}
/* deuxième étape */
@keyframes theFade {
    0% {opacity:0;}
    50% {opacity:0.5;}
    100% {opacity:1}
}

main.expanded{
	width:98%;
	padding-left:0;
	padding-top:0;
	padding-bottom:2rem;
}

main h1, main h2, main h3, main h4, main h5, main h6, main article, main p, main form {
	padding:0 1rem!important;
}
main:after {
	content: "";
	display: block;
}

main article p{
margin:1rem auto!important;
}
main form[name="myform"] {
	padding:0!important;
}
main form h1, main form h2, main form h3, main form h4, main form h5, main form h6, main form p{
	padding-left:0!important;
	margin-left:0!important;
}

main.home {
	padding: 0.5rem 2rem 0 2rem;
	padding-bottom: 0.5rem;
	background-color: transparent;
	box-shadow: none;
	animation-name: theFade; /* nom de l'animation */
	animation-duration: 1s; /* temps de l'animation (4s) */
	animation-iteration-count: 1; /* nombre de boucles (infinie) */
	animation-timing-function: linear; /* accelération de l'animation (constant) */
}
main.home article.disclamer p, main.home article h1 {
	font-family: "foobar_proregular", Arial, sans-serif;
	text-align: center;
}
main.home article h1 {
	margin:0;
}
