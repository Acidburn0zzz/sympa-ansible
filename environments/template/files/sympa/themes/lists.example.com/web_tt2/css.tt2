}

/* Links */

a {
	color: [% color_6 %];
	text-decoration: underline;
	transition: background-color 300ms ease-out;
}

a:visited {
	text-decoration: none;
}

a:focus {
	outline: 0;
	text-decoration: none;
}

a:hover, a:active,a:focus {
	color: [% color_6 %];
