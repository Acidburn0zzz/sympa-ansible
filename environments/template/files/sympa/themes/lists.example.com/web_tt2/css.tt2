            font-size: 1em;
    }
    h5 {
            font-size: 0.9em;
    }
    h6 {
            font-size: 0.8em;
    }
    .item_list .item .item_content ul.my li{
        display:block;
    }
    .item_list .item .item_content ul.my li:nth-child(2){
        padding-left:0;
    }
}

/* max-width 640px, mobile-only styles, use when QAing mobile issues */
@media only screen and (max-width: 40em) {
	main .title-bar {
		background: [% color_15 %];
	}
	main .title-bar .menu-icon {
		background: transparent;
	}
	main .top-bar, main .top-bar ul {
		background-color:[% color_13 %]
	}
	main nav .top-bar .top-bar-left, main nav .top-bar .top-bar-right {
		margin-left: 0;
	}
	main nav .top-bar .top-bar-right {
		width: 100%;
		position: absolute;
		z-index: 100;
		animation-name: theFade; /* nom de l'animation */
		animation-duration: 0.25s; /* temps de l'animation (4s) */
		animation-iteration-count: 1; /* nombre de boucles (infinie) */
		animation-timing-function: linear; /* accelération de l'animation (constant) */
		box-shadow: rgba(0, 0, 0, 0.5) 0px 10px 10px -2px;
	}
	main nav ul.menu {
		width: 100%;
	}
	main nav ul.menu a label{
		cursor: pointer;
	}
	main nav .menu li.has-submenu ul.submenu{
		margin-left:0;
		padding-left:0;
		background:[% color_13 %]
	}
	main nav .drilldown .js-drilldown-back>a::before{
		border-color: transparent [% color_6 %] transparent transparent;
	}
	main nav  menu.top-bar-right div.is-drilldown ul.drilldown li{
		width:100%;
		background-color:[% color_13 %];
		padding: 0;
		color:[% color_6 %];
	}
	main nav  menu.top-bar-right div.is-drilldown .drilldown a{
		background-color:[% color_13 %];
	}
	main nav  menu.top-bar-right div.is-drilldown ul.drilldown li:hover,main nav  menu.top-bar-right div.is-drilldown .drilldown a:hover{
		background-color:[% color_15 %];
		color:[% color_13 %];
	}
	main nav .top-bar .top-bar-right > * label {
		color: [% color_6 %];
		display:inline-block;
	}
	main nav .menu.icon-top > li > a img,
	main nav .menu.icon-top > li > a i,
	main nav .menu.icon-top > li > a span {
		display: inline-block;
		margin: 0 0.25rem;
	}
	main nav .is-drilldown {
		min-height: auto !important;
		width:100%;
	}
	main nav .is-drilldown li.hide-for-small-only {
		display: none!important;
	}
	main nav .is-drilldown li.show-for-small-only {
		display: block!important;
	}

	main nav  .drilldown .is-drilldown-submenu-parent > a::after {
		position: relative;
		right: auto;
		left:15%;
		display: inline-block;
		border-color: transparent transparent transparent [% color_6 %];
		margin-top: -0.7rem;
	}
	main.expanded{
		width:100%;
		padding-right:0;
	}
	nav .has-tip {
		border-bottom: none;
		font-weight: normal;
		cursor: auto;
	}
	main nav menu.top-bar-right div.is-drilldown ul.drilldown.letters > li.nolink{
		padding-left:0.5rem;
		padding-right:0.5rem;
		color:#000;
	}
}
@media only screen and (min-width: 641px) {
	main .top-bar, main .top-bar ul {
		background-color:[% color_15 %]
	}
	aside menu{
		max-width:100%;
		margin: 0;
		padding:0;
	}
	main aside {
		min-height:100vh;
		font-size: 0.9em;
		word-wrap: break-word;
		background: [% color_5 %];
		padding:0!important;
	}
	aside li {
		border: none;
		line-height: 1.7;
		list-style-type:none;
		font-weight:600;
	}
	aside li a {
		width:100%;
		display:inline-block;
		padding:0.5rem 0.5rem 0.5rem 1rem;
		font-weight:600;
	}
	aside li:hover, aside li a hover, aside li ul.dropdown li:hover{
		background-color:[% color_6 %];
		text-decoration:none!important;
	}
	aside li.has-dropdown:hover, aside li a hover{
		background-color:transparent;
	}
	aside menu ul {
		padding-left:0;
	}
	aside menu ul li.nolink {
		padding:0.5rem 0.5rem 0.5rem 1rem;
	}

	aside menu ul li ul li{
		padding:0 0 0 1.5rem;
	}
	aside menu ul li ul{
		margin-top: 0;
		margin-left:0;
		padding-left: 0;
	}


}
